<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="packages.gui.AjouterCommandeController">
   <children>
      <Label layoutX="201.0" layoutY="14.0" text="Ajouter une Commande">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="149.0" text="Date de commande" />
      <Label layoutX="51.0" layoutY="192.0" text="Quantité commandée" />
      <DatePicker fx:id="datecmd" layoutX="313.0" layoutY="145.0" />
      <Spinner fx:id="qtecmd" layoutX="313.0" layoutY="188.0" prefHeight="25.0" prefWidth="105.0" />
      <Button fx:id="btn_listcmd" layoutX="101.0" layoutY="316.0" mnemonicParsing="false" onAction="#afficherListeCommandes" text="Liste Commandes" />
      <Button fx:id="btn_ajoutcmd" layoutX="245.0" layoutY="316.0" mnemonicParsing="false" onAction="#ajouterCommande" prefHeight="25.0" prefWidth="110.0" text="Ajouter" />
      <Button fx:id="btn_listpr" layoutX="391.0" layoutY="316.0" mnemonicParsing="false" onAction="#afficherListeProduits" prefHeight="25.0" prefWidth="110.0" text="Liste des Produits" />
